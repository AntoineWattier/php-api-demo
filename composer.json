{
    "name": "antoinewattier/phpapidemo",
    "description": "PHP API Demo.",
    "keywords": ["php", "api"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.2.*",
        "vlucas/phpdotenv": "~2.2",
        "league/fractal": "^0.13.0",
